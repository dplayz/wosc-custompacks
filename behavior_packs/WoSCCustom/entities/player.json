{
  "minecraft:entity": {
    "format_version": "1.2.0",
    "component_groups": {
      
   "minecraft:normal_player": {
        "minecraft:health": {
          "value": 20,
          "max": 20
        },
        "minecraft:movement": {
          "value": 0.1 
        },
        "minecraft:attack": {
          "damage": 1
        },
        
        "minecraft:scale": {
        	"value": 1
        },
        "minecraft:player.saturation": {
          "value": 20
        },
        "minecraft:physics": {
          "has_gravity": true
        },
        "minecraft:type_family": {
          "family": [ "player", "undead" ]
        },
        "minecraft:fire_immune": false
      },
      
 "minecraft:attack_ring": {
        "minecraft:attack": {
          "damage": 8
        },
        "minecraft:movement": {
          "value": 0.1
        }
      },
      
      "minecraft:speed_ring": {
        "minecraft:attack": {
          "damage": 2
        },

        "minecraft:movement": {
          "value": 0.2
        }
      },

      "minecraft:life_ring": {
        "minecraft:attack": {
          "damage": 4
        },
        
        "minecraft:health":{
          "value":30,
          "max":30
        },

        "minecraft:movement": {
          "value": 0.1
        }
      }
    },
    
    "components": {
      "minecraft:identifier": {
        "id": "minecraft:player"
      },
      "minecraft:loot": {
        "table": "loot_tables/empty.json"
      },
      "minecraft:breathable": {
        "totalSupply": 20,
        "suffocateTime": -1,
        "generatesBubbles": false
      },
      "minecraft:collision_box": {
        "width": 0.6,
        "height": 1.8
      },
      "minecraft:can_climb": {
      },
      "minecraft:player.exhaustion": {
        "value": 0,
        "max": 4
      },
      "minecraft:player.level": {
        "value": 0,
        "max": 24791
      },
      "minecraft:player.experience": {
        "value": 0,
        "max": 1
      },
      "minecraft:nameable": {
        "alwaysShow": true,
        "allowNameTagRenaming": false
      },
      "minecraft:rideable": {
        "seat_count": 2,
        "family_types": [
          "parrot_tame"
        ],
		  "pull_in_entities": true,
        "seats": [
          {
            "position": [ 0.4, -0.15, 0.04 ],
            "min_rider_count": 0,
            "max_rider_count": 0,
            "lock_rider_rotation": 0
          },
          {
            "position": [ -0.4, -0.15, 0.04 ],
            "min_rider_count": 1,
            "max_rider_count": 2,
            "lock_rider_rotation": 0
          }
        ]
      },
      
      "minecraft:environment_sensor": [
        {
          "on_environment": {
            "filters": { 
              "all_of": [ 
                { "test": "brightness_greater", "subject": "self", "value": 0 }
              ]
            },
            "event": "minecraft:become_normal"
          }
        },

        {
          "on_environment": {
            "filters": { 
              "all_of": [ 
                { "test": "has_equipment", "subject": "self", "domain": "hand", "value": "rings:speed_ring"  }
              ]
            },
            "event": "minecraft:speed_ring"
          }
        },
        
        {
          "on_environment": {
            "filters": { 
              "all_of": [ 
                { "test": "has_equipment", "subject": "self", "domain": "hand", "value": "rings:life_ring"  }
              ]
            },
            "event": "minecraft:life_ring"
          }
        },
        
        {
          "on_environment": {
            "filters": { 
              "all_of": [ 
                { "test": "has_equipment", "subject": "self", "domain": "hand", "value": "rings:attack_ring"  }
              ]
            },
            "event": "minecraft:attack_ring"
          }
        }
      ]
    },
    "events":{
      "minecraft:become_normal": { 
        "add": { 
          "component_groups": [ 
            "minecraft:normal_player"
          ] 
        }, 
        "remove": { 
          "component_groups": [ 
      "minecraft:speed_ring", "minecraft:life_ring", "minecraft:attack_ring"
          ] 
        } 
      },
      
      "minecraft:attack_ring": { 
        "add": { 
          "component_groups": [ 
            "minecraft:attack_ring" 
          ] 
        }
      },

      "minecraft:speed_ring": { 
        "add": { 
          "component_groups": [ 
            "minecraft:speed_ring" 
          ] 
        }
      },

      "minecraft:life_ring": { 
        "add": { 
          "component_groups": [ 
            "minecraft:life_ring" 
          ]
        }
      }
    }
  }
}